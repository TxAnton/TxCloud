#include <stdio.h>

#define LENGHT 100

int MaxElement(int *array, int n);
int MinElement(int *array, int n);	
int DifferenceBetweenMaxAndMin(int *array, int n);
int SumElementsBeforeFirstMin(int *array, int n);

int main(){
    int number;
    int n=0;
    int array[LENGHT];
    int c;
    scanf("%d",&number);
	while(1){
		c=scanf("%d",array+n);
		if(c==-1){
			break;
		}
		n++;
	}
    switch(number) {
        case 0:
            printf("%d", MaxElement(array,n));
            break;
        case 1:
            printf("%d", MinElement(array,n));
            break;
        case 2:
            printf("%d", DifferenceBetweenMaxAndMin(array,n));
            break;
        case 3:
            printf("%d", SumElementsBeforeFirstMin(array,n));
            break;
    }
    if ((number < 0) || (number >= 4)) {
        printf("Данные некорректны");
    }
	return 0;
}

int MaxElement(int *array, int n) {
    int j;
    int max;
    max=array[0];
    for (j=1; j<n; j++) {
        if (array[j]>max) {
            max=array[j];
        }
    }
	return max;
}

int MinElement(int *array, int n) {
    int j;
    int min;
    min=array[0];
    for (j=1; j<n; j++) {
        if (array[j]<min) {
            min=array[j];
        }
    }
    return min;
}

int DifferenceBetweenMaxAndMin(int *array, int n) {
    int diff;
    diff = MaxElement(array,n) - MinElement(array,n);
    return diff;
}

int SumElementsBeforeFirstMin(int *array, int n) {
    int sum;
    int j;
    sum=0;
    j=0;
    while (array[j]> MinElement(array,n)) {
        sum=sum+array[j];
        j++;
    }   
    return sum;
}
